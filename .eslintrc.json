{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "rules": {
    "import/no-unresolved": "off",
    "comma-dangle": [
      "error",
      "never"
    ],
    "max-len": "off",
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error",
          "log"
        ]
      }
    ]
  }
}
